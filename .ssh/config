  User root
  IdentityFile ~/.ssh/root@shqld.dev/id
  
Host shqld.dev
  User sho
  IdentityFile ~/.ssh/git@github.com/id

Host rpi1.virus.shqld.dev
  User sho
  IdentityFile ~/.ssh/git@github.com/id

